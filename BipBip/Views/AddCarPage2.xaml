<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="BipBip.Views.AddCarPage2"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <StackLayout BackgroundColor="White">
        <Button Clicked="OnPreviousButtonClicked" Text="←" />

        <Label
            HorizontalOptions="CenterAndExpand"
            Text="Ajouter une voiture - Page 2"
            VerticalOptions="CenterAndExpand" />

        <Entry x:Name="TypeVehiculeEntry" Placeholder="Type de véhicule" />


        <Button Clicked="OnNextButtonClicked" Text="Suivant" />

    </StackLayout>
</ContentPage>
